#Create a class Student with name, age, and marks. Create two objects and print their details.
class Student:
    def __init__(self,name="",age=0,marks=""):
     self.name=name
     self.age=age
     self.marks=marks
    
    def getdata(self):
     self.name=input("Enter name of student:")
     self.age=int(input("Enter age of student:"))
     self.marks=int(input("Enter marks of student:"))
    
    def display(self):
     print(f"Student name is {self.name}")
     print(f"Student age is {self.age}")
     print(f"Student marks is {self.marks}")
    
